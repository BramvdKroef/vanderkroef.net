
// 100 = 50 + x ^ 2
digraph calc {
        graph [bgcolor="#FFFFFF",color="#4FA97B"]
        edge [color="#4FA97B"]
        node [shape=ellipse,width=.4,height=.4,color="#4FA97B",fontcolor="#333333",fontsize=12,fixedsize=true]
        100 -> equals
        50 -> plus -> equals
        x -> power -> plus
        2 -> power

        equals[label="="]
        plus[label="+"]
        power[label="^",color=red]
        100[color=red]
        2[color=red]
}
