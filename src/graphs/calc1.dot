// y = 20
// 5 * y = 50 + x * x
digraph calc {
        graph [bgcolor="#101010",color="#A4A4A4"]
        edge [color="#A4A4A4"]
        node [shape=ellipse,width=.4,height=.4,color="#A4A4A4",fontcolor="#A4A4A4",fontsize=12,fixedsize=true]
        5 -> times -> equals
        y -> times
        50 -> plus -> equals
        x1 -> times2 -> plus
        x2 -> times2

        times[label="×"]
        equals[label="="]
        times2[label="×"]
        plus[label="+"]
        x1[label="x"]
        x2[label="x"]
}


